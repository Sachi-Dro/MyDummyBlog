<%-include("partials/header")-%>
<h1>Compose</h1>


<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control"  name="ComposeTitle">
  </div>


  <div class="form-group">
    <label>Post</label>
    <textarea type="text" class="form-control"  name="ComposePost" cols="35" rows="7"></textarea>
